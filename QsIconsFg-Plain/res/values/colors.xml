<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Substratum QS exposures -->
    <bool name="config_enable_qs_tile_tinting">true</bool>
    <color name="qs_tiles_active_tint">@*android:color/accent_device_default_light</color>
    <color name="qs_tiles_inactive_tint">@*android:color/secondary_text_default_material_light</color>
    <!-- keep in mind that the code adds 0.26 (light) or 0.30 (dark) alpha to this color. -->
    <color name="qs_tiles_unavailable_tint">@*android:color/secondary_text_default_material_light</color>

    <color name="qs_tiles_background_active_tint">@android:color/transparent</color>
    <color name="qs_tiles_background_disabled_tint">@android:color/transparent</color>
    <color name="qs_tiles_background_inactive_tint">@android:color/transparent</color>
</resources>
