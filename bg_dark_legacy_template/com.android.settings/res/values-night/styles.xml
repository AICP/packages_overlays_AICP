<?xml version="1.0" encoding="utf-8"?>
<resources>
     <style name="Theme.Settings.Home" parent="@*com.android.settings:Theme.Settings.HomeBase">
        <item name="*com.android.settings:colorPrimary">@*android:color/primary_device_default_settings</item>
        <item name="*com.android.settings:colorPrimaryDark">@*android:color/primary_dark_device_default_settings</item>
    </style>
    <style name="ThemeOverlay.Settings.SearchBar" parent="@*com.android.settings:Theme.Settings">
        <item name="*com.android.settings:cardBackgroundColor">?android:attr/colorPrimary</item>
        <item name="android:colorControlNormal">?android:attr/colorAccent</item>
        <item name="android:colorBackground">?android:attr/colorPrimary</item>
    </style>
    <style name="EntityHeader">
        <item name="android:background">?android:attr/colorPrimary</item>
        <item name="android:paddingTop">24dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingEnd">16dp</item>
    </style>
    <style name="Widget.ActionBar.Base" parent="@android:style/Widget.DeviceDefault.ActionBar.Solid">
        <item name="android:background">?android:attr/colorPrimary</item>
    </style>
</resources>
